<% provide(:title, SIGNUP_PAGE_TITLE) %>
<% provide(:header, SIGNUP_PAGE_HEADER) %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'users/fields', form: f %>
      <%= f.submit "Create Account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
